@startuml
!theme plain
hide empty members

package "Data Structures" {
    class ttak_map_t {
        + buckets : ttak_bucket_t**
        + capacity : size_t
        + count : size_t
    }

    class ttak_shard_t {
        + data : void*
        + lock : ttak_rwlock_t
        __
        + init(data)
        + destroy()
    }

    class ttak_ringbuf_t {
        + buffer : void*
        + head : size_t
        + tail : size_t
        + capacity : size_t
        + full : bool
        + lock : ttak_rwlock_t
        __
        + push(item)
        + pop(out_item)
    }

    class ttak_object_pool_t {
        + buffer : void*
        + bitmap : uint8_t*
        + item_size : size_t
        + capacity : size_t
        + lock : ttak_spin_t
        __
        + alloc()
        + free(ptr)
    }

    class ttak_heap_t {
        + data : void**
        + size : size_t
    }

    class ttak_queue_t {
         + head : node_t*
         + tail : node_t*
    }
}
@enduml