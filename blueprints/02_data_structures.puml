@startuml
!theme plain
hide empty members

package "Hash Table (ht/)" {
    class ttak_map_t {
        + table : ttak_table_t*
        + hash_func : hash_fn
        __
        + create()
        + put(key, value)
        + get(key)
    }

    class ttak_table_t {
        + entries : ttak_table_entry_t**
        + capacity : size_t
        + size : size_t
    }
}

package "Containers (container/)" {
    class ttak_set_t {
        + map : ttak_map_t*
        __
        + add(member)
        + contains(member)
    }

    class ttak_pair_t {
        + first : void*
        + second : void*
    }

    class ttak_ringbuf_t {
        + buffer : void**
        + head : size_t
        + tail : size_t
        + capacity : size_t
        + lock : ttak_rwlock_t
    }

    class ttak_object_pool_t {
        + buffer : void*
        + bitmap : uint8_t*
        + item_size : size_t
        + capacity : size_t
    }
}
@enduml